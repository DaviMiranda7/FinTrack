  pod 'FirebaseFirestore'
  pod 'FirebaseMessaging'
  
  # UI e Charts
  pod 'Charts'
  
  # Networking
  pod 'Alamofire'
  
  # Utilit√°rios
  pod 'SwiftKeychainWrapper'

end
